<%
=begin%>
 <h1>Edit Profile Form</h1>
<%= form_with model: @profile, url: edit_profile(current_user.id) do |f| %>
<%= f.label :dob, "Birthday" %>
<%= f.date_field :dob, class: "form-control" %>
<%= f.label :bio, "Bio" %>
<%= f.text_field :bio, class: "form-control" %>
<%= f.label :address, "Current Location" %>
<%= f.text_field :address, class: "form-control" %>
<%= f.label :hobbies, "Hobbies" %>
<%= f.text_field :hobbies, class: "form-control" %><br>
<%= f.submit "Save Profile", class: "btn btn-primary" %>
<%end%> 
<%
=end%>